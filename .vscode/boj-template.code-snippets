      "const readline = require('readline');\n",
      "const rl = readline.createInterface({",
      "\tinput: process.stdin,",
      "\toutput: process.stdout,",
      "});\n",
      "const solution = (input) => {",
      "\tlet answer = 0;",
      "\treturn answer;",
      "};\n",
      "rl.on('line', (answer) => {",
      "\tconsole.log(solution(answer));",
      "\trl.close();",
      "});"
